<h1>Sign up</h1>

<form action="/insert_user" method="post">

  <div>
    <label for="email_box">Email</label>
    <input id="email_box" name="query_email" placeholder="Choose a email..." type="text">
  </div>
                 
  <div>
    <label for="username_box">Username</label>
      
    <input type="text" id="username_box" name="query_username">
  </div>
         
  <div>
    <label for="bio_box">Bio</label>
      
    <input type="text" id="bio_box" name="query_bio">
  </div>
         
  <div>
    <label for="city_box">City</label>
      
    <input type="text" id="city_box" name="query_city">
  </div>
         
  <div>
    <label for="name_box">Name</label>
      
    <input type="text" id="name_box" name="query_name">
  </div>
         
  <div>
    <label for="profilepic_box">Profilepic</label>
      
    <input type="text" id="profilepic_box" name="query_profilepic">
  </div>
         
  <div>
    <label for="private_box">Private</label>
    <input id="private_box" name="query_private" type="checkbox">
  </div>
         
  <div>
    <label for="sent_follow_requests_count_box">Sent follow requests count</label>
      
    <input type="text" id="sent_follow_requests_count_box" name="query_sent_follow_requests_count">
  </div>
         
  <div>
    <label for="received_follow_requests_count_box">Received follow requests count</label>
      
    <input type="text" id="received_follow_requests_count_box" name="query_received_follow_requests_count">
  </div>
         
  <div>
    <label for="own_plants_count_box">Own plants count</label>
      
    <input type="text" id="own_plants_count_box" name="query_own_plants_count">
  </div>
         
  <div>
    <label for="plantphotos_count_box">Plantphotos count</label>
      
    <input type="text" id="plantphotos_count_box" name="query_plantphotos_count">
  </div>
         
  <div>
    <label for="activities_count_box">Activities count</label>
      
    <input type="text" id="activities_count_box" name="query_activities_count">
  </div>
    
  <div>
    <label for="password_box">Password</label>
    <input id="password_box" name="query_password" placeholder="Choose a password..." type="password">
  </div>

  <div>
    <label for="password_confirmation_box">Password Confirmation</label>
    <input id="password_confirmation_box" name="query_password_confirmation" placeholder="Confirm your password..." type="password">
  </div>

  <button>
    Sign up
  </button>
</form>

<hr>

<p>
  Or, <a href="/user_sign_in">Sign in</a> instead.
</p>
